				message.execute(userMsgVisitor, (Void[])null);&#xD;
			}&#xD;
		}&#xD;
		&#xD;
		this.user = new User();&#xD;
		try {&#xD;
			localAddress = rmiUtils.getLocalAddress();&#xD;
		} catch (SocketException | UnknownHostException e) {&#xD;
			System.out.println(&quot;Error getting local address: &quot; + e.getClass());&#xD;
