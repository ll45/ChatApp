          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0S0n0zQiEei9npXO31xcNA" name="stop" specification="_0F6NMTQiEei9npXO31xcNA">
            <language>java</language>
            <body>try {&#xD;
			// Leave all current chatrooms&#xD;
			for (IChatroomModel chatroomModel : chatroomModels) {&#xD;
				IChatroom chatroom = chatroomModel.getChatroom();&#xD;
				chatroom.notifyAllReceivers(new ChatroomDataPacket&lt;Quit>(&#xD;
						Quit.class, new Quit(), chatroomModel.getReceiverStub()));&#xD;
			}&#xD;
			// Disconnect from all users&#xD;
			for (IUser connectedUser : connectedUsers) {&#xD;
				connectedUser.accept(new UserDataPacket&lt;IDisconnect>(IDisconnect.class, new Disconnect(), userStub));&#xD;
			}&#xD;
			// Kill the application&#xD;
			rmiUtils.stopRMI();&#xD;
			System.exit(0);&#xD;
		} catch (Exception e) {&#xD;
			System.exit(-1);&#xD;
		}&#xD;
	</body>
